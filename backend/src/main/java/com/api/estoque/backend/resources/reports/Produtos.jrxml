	<queryString>
		<![CDATA[SELECT
	estoque_entrada.id_estoque,
	produto.id_produto,
	produto.nome AS produto_nome,
	produto.status,
	unidade_medida.unidade_medida,
	fornecedor.name AS fornecedor_nome,
	filial.name AS filial_nome
FROM estoque_entrada
INNER JOIN item_estoque_entrada ON item_estoque_entrada.id_estoque = estoque_entrada.id_estoque
INNER JOIN produto ON produto.id_produto = item_estoque_entrada.id_produto
INNER JOIN unidade_medida ON unidade_medida.id_unidade_medida = produto.id_unidade_medida
INNER JOIN fornecedor ON fornecedor.id_fornecedor = item_estoque_entrada.id_fornecedor
INNER JOIN filial ON filial.id_filial = estoque_entrada.id_filial
WHERE filial.id_filial =  1
ORDER BY estoque_entrada.id_estoque ASC]]>
	</queryString>
